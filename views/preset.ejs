<div class="container-fluid bg-white">
    <div class="sid-wrapper">
         <input type="search" class ="form-control mb-3" id="preset_search" placeholder="search preset here" style="width:100%; display:flex; justify-content:center; " />
    </div>
    <div class="d-none d-block" id="preset-message">
       <div class="alert alert-info" style="font-size: 12px;">
          <div class="m" id="pm">message: </div>
          <div class="m" id="pt">tip: </div>
       </div>
    </div>
    <div class="sid-wrapper d-none p-2">
        <form class="p-2" style="overflow: auto; height: 500px;" id="msd-preset" scan="msd" state="preset"   >
             <label for="">title</label>
             <input type="text" class="form-control" id="title-preset">
            <label for="">location</label>
            <select name="location-preset" id="location-preset" class="form-control">
               <option value="extrauterine">extrauterine</option>
               <option value="intrauterine">intrauterine</option>
               <option value="heterotopic">heterotopic</option>
            </select>
            <label for="">yolk sac</label>
            <select name="yolk-preset" id="yolk-preset" class="form-control">
               <option value="absent">absent</option>
               <option value="not yet">not yet</option>
               <option value="present">present</option>
            </select>
            <label for="">ovaries</label>
            <textarea name="ovaries-preset" id="ovaries-preset" cols="30" rows="4" placeholder="type something..."></textarea>
            <label for="">adnexa</label>
            <textarea name="adnexa-preset" id="adnexa-preset" cols="30" rows="4" placeholder="type something..."></textarea>
            <label for="">abnormal findings</label>
            <textarea name="abnormal-preset" id="abnormal-preset" cols="30" rows="4" placeholder="type something..."></textarea>
            <label for="">impression</label>
            <textarea name="impression-preset" id="impression-preset" cols="30" rows="4" placeholder="type something..."></textarea>
            <button type="button" class="btn btn-outline-primary" id="savepreset">save</button>
            <button type="button" class="btn btn-outline-info" id="clearpreset">clear</button>
         </form>
    </div>
    <div class="sid-wrapper ">
       <div class="list-group" id="preset-group">
         
       </div>
       </div>
    </div>

</div>
